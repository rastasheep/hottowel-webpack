{
  "name": "hottowel-webpack",
  "description": "Project Generated from HotTowel Angular",
  "version": "0.0.0",
  "scripts": {
    "init": "npm install",
    "start": "node src/server/app.js",
    "test": "npm run test:server && npm run test:client",
    "test:client": "karma start",
    "test:server": "echo 'TODO: test:server'",
    "build": "rimraf build && webpack",
    "serve-dev": "node ./script/dev-server.js",
    "vet": "npm run vet:server && npm run vet:client && npm run vet:styles",
    "vet:server": "echo 'TODO: vet:server'",
    "vet:client": "eslint -c .eslintrc.client.json src/client",
    "vet:styles": "stylelint 'src/client/**/*.scss' --syntax scss",
    "analysis": "npm run analysis:client && npm run analysis:server",
    "analysis:client": "plato -t 'Client' -r -d ./report/plato/client -e .eslintrc.client.json -x .spec.js ./src/client/app/**/*.js",
    "analysis:server": "plato -t 'API' -r -d ./report/plato/api -x .spec.js ./src/server/**/*.js"
  },
  "dependencies": {
    "body-parser": "^1.16.1",
    "express": "^4.14.1",
    "morgan": "^1.8.1",
    "serve-favicon": "^2.0.1"
  },
